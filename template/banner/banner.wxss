/*banner.wxss  */
.banner {
    display: block;
    height: 345rpx;
}
.banner .banner-image{
    width: 100%;
    height: 100%;
}
.banner .default-banner-image{
    width: 60rpx!important;
    height: 60rpx!important;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
